{
  "appId": "domain.company-name.app-name",
  "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
  "nsis": {
    "uninstallDisplayName": "${productName}"
  },
  "mac": {
    "target": "dmg",
    "category": "mac.app.category"
  },
  "linux": {
    "target": "AppImage",
    "category": "Category"
  },
  "win": {
    "target": "nsis"
  }
}
